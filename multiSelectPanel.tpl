<div class="multiSelectPanel">
    <div class="multiSelectPanel-wrapper cf">
        <div class="multiSelectPanel-left">
            <div class="multiSelectPanel-left-top">
                <span class="multiSelectPanel-left-title"></span>
                <input type="checkbox" class="itemSelectAll" id="allItem"/>
                <label for="allItem"></label>
            </div>
            <div class="multiSelectPanel-left-search">
                <input class="multiSelectPanel-searchBox" type="text" placeholder="输入关键字查询"/>
                <a href="" class="multiSelectPanel-searchBox-btn"></a>
            </div>
            <div class="multiSelectPanel-left-panel">
                <ul class="multiSelectPanel-left-panel-ul">
                    <li>客服1 <input type="checkbox" class="itemCheckbox" id="1"/><label for="1"></label></li>
                    <li>客服2 <input type="checkbox" class="itemCheckbox" id="2"/><label for="2"></label></li>
                    <li>客服3 <input type="checkbox" class="itemCheckbox" id="3"/><label for="3"></label></li>
                    <li>客服4 <input type="checkbox" class="itemCheckbox" id="4"/><label for="4"></label></li>
                </ul>
            </div>
            <div class="multiSelectPanel-left-bottom">
                共<span class="itemTotalNum"></span>个<span class="multiSelectPanel-left-bottom-title"></span>
            </div>
        </div>
        <div class="multiSelectPanel-right">
            <div class="multiSelectPanel-right-top">
                <span class="multiSelectPanel-title">已选择</span>
                <input type="checkbox" class="itemDeleteAll" id="deleteAll"/>
                <label for="deleteAll"></label>
            </div>
            <div class="multiSelectPanel-right-search">
                <input class="multiSelectPanel-searchBox" type="text" placeholder="输入关键字查询"/>
                <a href="" class="multiSelectPanel-searchBox-btn"></a>
            </div>
            <div class="multiSelectPanel-right-panel">
                <ul class="multiSelectPanel-right-panel-ul">
                    <li>客服1 <input type="checkbox" class="itemDelCheckbox" id="d1"/><label for="d1"></label></li>
                </ul>
            </div>
            <div class="multiSelectPanel-right-bottom">
                已选择<span class="itemTotalNum"></span>个
            </div>
        </div>
    </div>
    <div class="multiSelectPanel-bottom">
        <a href="" class="multiSelectPanel-saveItem">保存</a>
        <a href="" class="multiSelectPanel-deleteItem">取消</a>
    </div>
</div>